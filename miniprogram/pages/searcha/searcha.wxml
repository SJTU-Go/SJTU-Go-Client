<view class = 'outsideblock'>
<view class = 'marginn' style="height:{{height}}rpx;">

  <form class="form" bindsubmit="formSubmit">
<view class="section00">
<view class ='left'>
<view class="verticalline1"></view>
 <view class="verticalline4"></view>     
    </view>
     <view class = 'right'>
    <view class="section1">
      <view class = "sectionleft">
        <view class = 'emptyblock'></view>
        <view class = 'upsection'>From</view>
        <view class = 'downsection'>
         <view wx:if = "{{depart}}">
         <input bindtap="depart"    placeholder-style="color:rgb(5, 5, 5,0.651);" placeholder="{{depart}}" />
         </view>
         <view wx:else>
         <input bindtap="depart"    placeholder-style="color:rgb(5, 5, 5,0.651);" placeholder="当前位置" /></view>
        </view>
      </view> 
    <view wx:if="{{passnum<=2}}"class = 'iconright'>
      <image class="massge-img" src="../../images/add1.png" bindtap = 'addpass'></image></view>
    </view>  
    <view class="divLine"></view></view>
    </view>
    <view wx:for="{{[0, 1, 2, 3, 4, 5]}}" wx:for-item="i">
  <view wx:if="{{i < passnum}}">
  <view class="section0">
  <view class ='left'>

      <view class="verticalline2"></view>
    </view>
      <view class = 'right'>

    <view class="section">
      <view class = "sectionleft">
      <view class = 'emptyblock'></view>
      <view class = 'upsection'>Pass</view>
      <view class = 'downsection'>
         <view wx:if = "{{pass}}">
         <input bindtap="pass"    placeholder-style="color:rgb(5, 5, 5,0.651);" placeholder="{{pass}}" />
         </view>
         <view wx:else>
        <input bindtap="pass"    placeholder-style="color:rgb(5, 5, 5,0.651);" placeholder="点击输入..." />
        </view>
      </view>
     </view>
    <view class = 'iconright1'>
      <image class="massge-img" src="../../images/cross.png" bindtap = 'deletepass'></image></view>
     </view>  
   
    <view class="divLine"></view></view>
  </view></view></view>

  <view class="section03">
<view class ='left'>
<view class="verticalline22"></view>
      <view class="verticalline3"></view>
    </view>
     <view class = 'right'>
    <view class="section">
      <view class = "sectionleft">
      <view class = 'emptyblock'></view>
      <view class = 'upsection'>To</view>
      <view class = 'downsection'>
         <view wx:if = "{{arrive}}">
         <input bindtap="arrive"    placeholder-style="color:rgb(5, 5, 5,0.651);" placeholder="{{arrive}}" />
         </view>
         <view wx:else>
        <input bindtap="arrive"    placeholder-style="color:rgb(5, 5, 5,0.651);" placeholder="点击输入..." />
            </view>
      </view>
    </view>
    </view>  
    <view class="divLine"></view>
</view>
    </view> 
  </form>



</view>

</view>
<view class = 'flexbbox' style="margin-top:{{totop}}rpx;">
  <view class = 'fill'></view>
  <view class = 'others'>
    <checkbox-group bindchange="checkboxChange">
    <label class="checkbox" wx:for-items="{{checkInfo}}">
      <checkbox value="{{item.name}}" checked="{{item.checked}}"/>{{item.value}}
    </label>
    </checkbox-group>
  </view>
    
</view>
<view class="line" wx:if="{{settim}}">
<view class="tui-picker-content">
  <view class="tui-picker-name"></view>
  <picker mode="multiSelector" value="{{dateTime}}" bindchange="changeDateTime" bindcolumnchange="changeDateTimeColumn" range="{{dateTimeArray}}">
    <view class="tui-picker-detail">
      <text class="t" style="color:#00a278">选择开始时间:</text> {{dateTimeArray[0][dateTime[0]]}}-{{dateTimeArray[1][dateTime[1]]}}-{{dateTimeArray[2][dateTime[2]]}} {{dateTimeArray[3][dateTime[3]]}}:{{dateTimeArray[4][dateTime[4]]}}
    </view>
  </picker>
</view>
</view>

<view class = 'toolblock'>
<view class = 'downblock'>
  <button class='btn2' bindtap='formSubmit'>
    <view>重新搜索</view>
    </button>
  <button class='btn1' bindtap='indexback'>
    <view>回到主页</view>
    </button>
</view>
</view>

<view class='topTabSwiper'>
    <view class='tab  {{currentData == 0 ? "tabBorer" : ""}}'  data-current = "0" bindtap='checkCurrent'>偏好</view>
    <view class='tab  {{currentData == 1 ? "tabBorer" : ""}}'  data-current = "1" bindtap='checkCurrent'>总时间</view>
    <view class='tab  {{currentData == 2 ? "tabBorer" : ""}}'  data-current = "2" bindtap='checkCurrent'>步行距离</view>
    <view class='tab  {{currentData == 3 ? "tabBorer" : ""}}'  data-current = "3" bindtap='checkCurrent'>费用</view>
</view>

<view wx:if="{{currentData==0}}">
  <view class="container">
    <view class="massge" wx:for="{{preferencelist}}"  wx:for-item="list" wx:if="{{list.type}}">
      <view class="massge-hd">
        <view wx:if="{{list.type==method[0]}}"><image class="massge-img" src="../../images/foot.png"></image>
        </view>
        <view wx:elif="{{list.type==method[1]}}"><image class="massge-img" src="../../images/bus1.png"></image></view>
        <view wx:elif="{{list.type==method[2]}}"><image class="massge-img" src="../../images/bike.png"></image></view>
        <view wx:elif="{{list.type==method[3]}}"><image class="massge-img" src="../../images/car.png"></image></view>
        <view wx:if="{{list.type==method[0]}}"  class='walkbutton'>{{list.type}}</view>
        <view wx:if="{{list.type==method[1]}}"  class='busbutton'>{{list.type}}</view>
        <view wx:if="{{list.type==method[2]}}"  class='bikebutton'>{{list.type}}</view>
        <view wx:if="{{list.type==method[3]}}"  class='carbutton'>{{list.type}}</view>
        <view class = 'rightt'>
          <view wx:if="{{list.cost}}" class='textclass'><text>{{list.travelTime/60|Int}} min · {{list.cost/60}}元</text></view>
          <view wx:else class='textclass'><text>{{list.travelTime/60|Int}} min · 免费</text></view>
        </view>
      </view>
      <view class='divLine'></view>
      <view class="massge-bd font-color-666">
       <view> 
        <text>需要步行<text class="font-color-c24e4e font-size-15">{{list.distance}}米，预计花费{{list.travelTime/60|Int}} min。</text></text></view>

      <view wx:if="{{list.type==method[1]}}">   <view class='textdiv'><text bindtap="searchPagebus">路径详情></text></view></view>
      <view wx:if="{{list.type==method[0]}}">   <view class='textdiv'><text bindtap="searchPagewalk">路径详情></text></view></view>
      <view wx:if="{{list.type==method[2]}}">   <view class='textdiv'><text bindtap="searchPagebike">路径详情></text></view></view>
      <view wx:if="{{list.type==method[3]}}">   <view class='textdiv'><text bindtap="searchPageCar">路径详情></text></view></view>
      </view>

    </view>
  </view>
</view>
<view wx:if="{{currentData==1}}">
  <view class="container">
    <view class="massge" wx:for="{{value}}"  wx:for-item="list" wx:if="{{list.type}}">
      <view class="massge-hd">
        <view wx:if="{{list.type==method[0]}}"><image class="massge-img" src="../../images/foot.png"></image>
        </view>
        <view wx:elif="{{list.type==method[1]}}"><image class="massge-img" src="../../images/bus1.png"></image></view>
        <view wx:elif="{{list.type==method[2]}}"><image class="massge-img" src="../../images/bike.png"></image></view>
        <view wx:elif="{{list.type==method[3]}}"><image class="massge-img" src="../../images/car.png"></image></view>
        <view wx:if="{{list.type==method[0]}}"  class='walkbutton'>{{list.type}}</view>
        <view wx:if="{{list.type==method[1]}}"  class='busbutton'>{{list.type}}</view>
        <view wx:if="{{list.type==method[2]}}"  class='bikebutton'>{{list.type}}</view>
        <view wx:if="{{list.type==method[3]}}"  class='carbutton'>{{list.type}}</view>
        <view class = 'rightt'>
          <view wx:if="{{list.cost}}" class='textclass'><text>{{list.travelTime/60|Int}} min · {{list.cost/60}}元</text></view>
          <view wx:else class='textclass'><text>{{list.travelTime/60|Int}} min · 免费</text></view>
        </view>
      </view>
      <view class='divLine'></view>
      <view class="massge-bd font-color-666">
       <view> 
        <text>需要步行<text class="font-color-c24e4e font-size-15">{{list.distance}}米，预计花费{{list.travelTime/60|Int}} min。</text></text></view>

      <view wx:if="{{list.type==method[1]}}">   <view class='textdiv'><text bindtap="searchPagebus">路径详情></text></view></view>
      <view wx:if="{{list.type==method[0]}}">   <view class='textdiv'><text bindtap="searchPagewalk">路径详情></text></view></view>
      <view wx:if="{{list.type==method[2]}}">   <view class='textdiv'><text bindtap="searchPagebike">路径详情></text></view></view>
        <view wx:if="{{list.type==method[3]}}">   <view class='textdiv'><text bindtap="searchPageCar">路径详情></text></view></view>    
      </view>

    </view>
  </view>
</view>

